{
    "name": "Get IPs",
    "version": "1.2.0",
    "description": "Get Server & Client IPs.",
    "permissions": [
        "clipboardWrite",
        "dns",
        "tabs",
        "<all_urls>",
        "http://*/*",
        "https://*/*"
    ],
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/logo/getips-16.png",
            "32": "images/logo/getips-32.png",
            "48": "images/logo/getips-48.png",
            "128": "images/logo/getips-128.png"
        },
        "default_title": "Get IPs"
    },
    "icons": {
        "16": "images/logo/getips-16.png",
        "32": "images/logo/getips-32.png",
        "48": "images/logo/getips-48.png",
        "128": "images/logo/getips-128.png"
    }
}