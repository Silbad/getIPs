{
    "name": "get-ips",
    "version": "1.2.0",
    "description": "WebExtension to Get Server & Client IPs",
    "license": "MPL-2.0",
    "author": "Silbad",
    "repository": {
        "type": "git",
        "url": "https://github.com/Silbad/getIPs"
    },
    "bugs": {
        "url": "https://github.com/Silbad/getIPs/issues"
    },
    "scripts": {
        "code:lint": "eslint '**/**/*.{ts,tsx,svelte}'",
        "code:lint-fix": "eslint --fix '**/**/*.{ts,tsx,svelte}'",
        "code:validate": "svelte-check",
        "code:typecheck": "./node_modules/.bin/tsc --noEmit",
        "code:prettier": "prettier --write '**/**/*.{ts,tsx,svelte}'",
        "css:lint": "stylelint 'src/assets/**/**/*.css'",
        "css:lint-fix": "stylelint 'src/assets/**/**/*.css' --fix",
        "app:chrome": "rimraf dist && cross-env BROWSER='chrome' webpack --config webpack/webpack.prod.js",
        "app:chrome-dev": "cross-env BROWSER='chrome' webpack --config webpack/webpack.dev.js --watch",
        "app:edge": "rimraf dist && cross-env BROWSER='edge' webpack --config webpack/webpack.prod.js",
        "app:edge-dev": "cross-env BROWSER='edge' webpack --config webpack/webpack.dev.js --watch",
        "app:firefox": "rimraf dist && cross-env BROWSER='firefox' webpack --config webpack/webpack.prod.js",
        "app:firefox-dev": "cross-env BROWSER='firefox' webpack --config webpack/webpack.dev.js --watch",
        "test:unit": "jest",
        "test:unit-update-snapshot": "jest --updateSnapshot"
    },
    "dependencies": {
        "axios": "^0.26.1",
        "ipaddr.js": "^2.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.14.2",
        "@babel/preset-env": "^7.14.2",
        "@babel/preset-typescript": "^7.13.0",
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/svelte": "^3.0.3",
        "@tsconfig/svelte": "^1.0.10",
        "@types/chrome": "0.0.139",
        "@types/jest": "^26.0.23",
        "@types/node": "^14.14.45",
        "@types/sinon": "^10.0.0",
        "@types/testing-library__jest-dom": "^5.9.5",
        "@typescript-eslint/eslint-plugin": "^4.23.0",
        "@typescript-eslint/parser": "^4.23.0",
        "babel-jest": "^26.6.3",
        "copy-webpack-plugin": "^8.1.1",
        "cross-env": "^7.0.3",
        "css-loader": "^5.2.4",
        "eslint": "^7.26.0",
        "eslint-plugin-import": "^2.23.0",
        "eslint-plugin-svelte3": "^3.2.0",
        "git-hooks-plus": "^1.0.1",
        "jest": "^27.5.1",
        "mini-css-extract-plugin": "^2.6.0",
        "prettier": "^2.6.2",
        "prettier-plugin-svelte": "^2.7.0",
        "rimraf": "^3.0.2",
        "sinon": "^10.0.0",
        "stylelint": "^13.13.1",
        "stylelint-config-standard": "^22.0.0",
        "svelte": "^3.47.0",
        "svelte-check": "^2.7.0",
        "svelte-jester": "^2.3.2",
        "svelte-loader": "^3.1.2",
        "svelte-preprocess": "^4.10.6",
        "ts-jest": "^26.5.6",
        "ts-loader": "^8.2.0",
        "tslib": "^2.0.1",
        "typescript": "^4.2.4",
        "web-ext-types": "^3.2.1",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "whatwg-fetch": "^3.6.2"
    }
}
